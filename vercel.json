{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST"],
      "dest": "src/index.ts"
    }
  ],
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'none'; img-src 'self' https://circle-backend-theta.vercel.app; script-src 'self'; style-src 'self';"
        }
      ]
    }
  ]
}
